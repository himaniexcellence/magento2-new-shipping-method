<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
	<default>
    <carriers>
      <shippingmethod>
        <enable>1</enable>
        <sallowspecific>0</sallowspecific>
        <model>Excellence\Shipping\Model\Carrier\ShippingMethod</model>
        <name>Free Delivery</name>
        <price>50.00</price>
        <title>Free Delivery</title>
        <type>I</type>
        <specific_error_msg>This shipping method is not available. To use this shipping method, please contact us.</specific_error_msg>
      </shippingmethod>
    </carriers>
  </default>
</config>